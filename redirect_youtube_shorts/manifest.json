{
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/shorts/*"],
      "js": ["redirect_youtube_shorts.js"]
    }
  ],
  "description": "Redirects YouTube shorts to their regular video.",
  "homepage_url": "https://github.com/honzi/Firefox-Extensions.js",
  "manifest_version": 2,
  "name": "redirect_youtube_shorts",
  "version": "1.0.1"
}
